{
  "typescript.tsdk": "node_modules/typescript/lib",
  "debug.node.autoAttach": "on",
  "debug.javascript.autoAttachFilter": "smart",
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "<node_internals>/**",
      "**/node_modules/**"
    ]
  },
  "files.exclude": {
    "**/dist": false
  },
  "search.exclude": {
    "**/dist": true,
    "**/node_modules": true
  }
}
