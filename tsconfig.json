{
  "compilerOptions": {
    "target": "ES5",
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "jsx": "react",
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": false,
    "noUnusedLocals": false,
    "paths": {
      "*": ["node_modules/*"],
      "@/*": ["src/main/*"],
      "common/*": ["src/common/*"]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/renderer/**/*"
  ],
  "extends": "./node_modules/electron-webpack/tsconfig-base.json"
}
